---
layout: false
title: Manual
---

<% wrap_layout :core do %>
  <% manual_index_page.columns.each do |column_pages| %>
    <% column_pages.each do |section_name, pages| %>
      <% next unless pages.any? %>
      <h2><%= section_name %></h2>
      <ul>
        <% pages.each do |page| %>
          <li><%= link_to page.data.title, page.path %></li>
        <% end %>
      </ul>
    <% end %>
  <% end %>
<% end %>
